fonction rechercheDicoto(in tab: [],in valeur: N, out position:N)
    var position : N <- -1
    var deb <- N:1
    var milieu <- N
    var fin <- N : longueur(tab)
    Tant que debut < fin
        milieu <- N : (debut+milieu) / 2
        si tab[milieu] > valeur
            alors
                fin <- milieu 
            sinon 
                debut <- milieu + 1
        finsi
        milieu <- (fin + debut ) / 2
    fintantque

    si valeur == tab[debut]
        alors   
            position = debut
    finsi
retourne position

